    <altmember cref="T:System.Data.OleDb.OleDbConnection" />
    <related type="Article" href="https://msdn.microsoft.com/library/f46570de-9e50-4fe6-8710-a8c31fa8569b">トランザクションの実行</related>
  </Docs>
